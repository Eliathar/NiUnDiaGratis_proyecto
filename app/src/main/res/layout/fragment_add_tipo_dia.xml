<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddTipoDiaFragment">

    <EditText
        android:id="@+id/editTextNombre13"
        android:layout_width="315dp"
        android:layout_height="57dp"
        android:layout_marginBottom="136dp"
        android:hint="Nombre tipo dia"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMax13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Spinner
        android:id="@+id/spinnerMax13"
        android:layout_width="325dp"
        android:layout_height="41dp"
        android:layout_marginBottom="256dp"
        app:layout_constraintBottom_toTopOf="@+id/btnGuardar13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnGuardar13"
        android:layout_width="271dp"
        android:layout_height="91dp"
        android:text="@string/guardar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.934" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="214dp"
        android:layout_height="35dp"
        android:text="@string/max_dias"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMax13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.218"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNombre13"
        app:layout_constraintVertical_bias="0.871" />
</androidx.constraintlayout.widget.ConstraintLayout>